        "browserify-zlib": "^0.2.0",
        "buffer": "^6.0.3",
        "chart.js": "^3.8.0",
        "chartjs-plugin-annotation": "^1.4.0",
        "coffeequate": "^1.3.0",
        "dagre": "^0.8.5",
        "jquery": "^3.6.0",
        "material-icons": "^1.12.2",
        "mathjs": "^10.0.0",
        "patch-package": "^6.4.7",
        "pixi-viewport": "4.24",
        "pixi.js": "6.1.3",
        "process": "^0.11.10",
        "stream-browserify": "^3.0.0"
    }

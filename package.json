        "@types/bootstrap": "^5.2.6",
        "@types/dagre": "^0.7.48",
        "@types/jest": "^29.5.3",
        "@types/jquery": "^3.5.16",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "autoprefixer": "^10.4.13",
        "babel-jest": "^29.6.1",
        "babel-loader": "^9.1.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "css-loader": "^6.7.3",
        "eslint": "^8.31.0",
        "jest": "^29.6.1",
        "node-sass": "^9.0.0",
        "postcss-loader": "^7.0.2",
        "rimraf": "^3.0.2",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",

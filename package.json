{
    "name": "@spcl/sdfv",
    "version": "1.3.0",
    "description": "A standalone viewer for SDFGs",
    "homepage": "https://github.com/spcl/dace-webclient",
    "main": "out/index.js",
    "types": "out/index.d.js",
    "keywords": [
        "sdfg",
        "dace",
        "dapp",
        "spcl"
    ],
    "engines": {
        "node": ">=20.17.0"
    },
    "bugs": {
        "url": "https://github.com/spcl/dace-webclient/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/spcl/dace-webclient.git"
    },
    "license": "BSD-3-Clause",
    "scripts": {
        "start": "npm run serve",
        "clean": "rimraf dist && rimraf out",
        "type-check": "tsc --noEmit",
        "build-dev": "npm run clean && webpack --config-name=main --mode=development",
        "watch": "npm run clean && webpack --watch --config-name=main --mode=development",
        "serve": "npm run clean && webpack serve --config-name=main --mode=development",
        "build-prod": "npm run clean && webpack --mode=production",
        "postinstall": "patch-package",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/cli": "^7.25.6",
        "@babel/core": "^7.25.2",
        "@babel/plugin-transform-class-properties": "^7.25.4",
        "@babel/plugin-transform-runtime": "^7.25.4",
        "@babel/preset-env": "^7.25.4",
        "@babel/preset-typescript": "^7.24.7",
        "@types/bootstrap": "^5.2.10",
        "@types/dagre": "^0.7.52",
        "@types/jest": "^29.5.12",
        "@types/jquery": "^3.5.30",
        "@types/lodash": "^4.17.7",
        "@typescript-eslint/eslint-plugin": "^8.4.0",
        "@typescript-eslint/parser": "^8.4.0",
        "autoprefixer": "^10.4.13",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "css-loader": "^7.1.2",
        "eslint": "^9.9.1",
        "file-loader": "^6.2.0",
        "jest": "^29.7.0",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "^29.7.0",
        "monaco-editor-webpack-plugin": "^7.1.0",
        "postcss-loader": "^8.1.1",
        "rimraf": "^6.0.1",
        "sass": "1.77.6",
        "sass-loader": "^16.0.1",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.25.6",
        "assert": "^2.1.0",
        "bootstrap": "^5.3.3",
        "browserify-zlib": "^0.2.0",
        "buffer": "^6.0.3",
        "chart.js": "^4.1.2",
        "chartjs-plugin-annotation": "^2.1.1",
        "coffeequate": "^1.3.0",
        "dagre": "^0.8.5",
        "jquery": "^3.7.1",
        "lodash": "^4.17.21",
        "material-symbols": "^0.22.2",
        "mathjs": "^13.1.1",
        "monaco-editor": "^0.51.0",
        "patch-package": "^8.0.0",
        "pixi-viewport": "4.24",
        "pixi.js": "6.1.3",
        "process": "^0.11.10",
        "stream-browserify": "^3.0.0"
    }
}

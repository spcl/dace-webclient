{
    "name": "@spcl/sdfv",
    "version": "1.4.0",
    "description": "A standalone viewer for SDFGs",
    "homepage": "https://github.com/spcl/dace-webclient",
    "main": "out/index.js",
    "types": "out/index.d.js",
    "keywords": [
        "sdfg",
        "dace",
        "dapp",
        "spcl"
    ],
    "engines": {
        "node": ">=22.17.0"
    },
    "bugs": {
        "url": "https://github.com/spcl/dace-webclient/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/spcl/dace-webclient.git"
    },
    "license": "BSD-3-Clause",
    "scripts": {
        "start": "npm run serve",
        "clean": "rimraf dist && rimraf out",
        "type-check": "tsc --noEmit",
        "build-dev": "npm run clean && webpack --config-name=main --mode=development",
        "watch": "npm run clean && webpack --watch --config-name=main --mode=development",
        "serve": "npm run clean && webpack serve --config-name=main --mode=development",
        "build-prod": "npm run clean && webpack --mode=production",
        "postinstall": "patch-package",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.0",
        "@babel/core": "^7.28.0",
        "@babel/plugin-transform-class-properties": "^7.27.1",
        "@babel/plugin-transform-runtime": "^7.28.0",
        "@babel/preset-env": "^7.28.0",
        "@babel/preset-typescript": "^7.27.1",
        "@eslint/js": "^9.32.0",
        "@types/bootstrap": "^5.2.10",
        "@types/d3": "^7.4.3",
        "@types/dagre": "^0.7.53",
        "@types/jest": "^30.0.0",
        "@types/jquery": "^3.5.32",
        "@types/lodash": "^4.17.20",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "autoprefixer": "^10.4.21",
        "babel-jest": "^30.0.5",
        "babel-loader": "^10.0.0",
        "css-loader": "^7.1.2",
        "eslint": "^9.32.0",
        "file-loader": "^6.2.0",
        "globals": "^16.3.0",
        "jest": "^30.0.5",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "^30.0.5",
        "monaco-editor-webpack-plugin": "^7.1.0",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "sass": "1.77.6",
        "sass-loader": "^16.0.5",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.4.1",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "typedoc": "^0.28.4",
        "typescript": "^5.8.0",
        "typescript-eslint": "^8.38.0",
        "webpack": "^5.101.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2"
    },
    "dependencies": {
        "@babel/runtime": "^7.28.2",
        "assert": "^2.1.0",
        "bootstrap": "^5.3.7",
        "browserify-zlib": "^0.2.0",
        "buffer": "^6.0.3",
        "chart.js": "^4.5.0",
        "chartjs-plugin-annotation": "^3.1.0",
        "chartjs-plugin-datalabels": "^2.2.0",
        "coffeequate": "^1.3.0",
        "d3": "^7.9.0",
        "dagre": "^0.8.5",
        "jquery": "^3.7.1",
        "lodash": "^4.17.21",
        "material-symbols": "^0.38.0",
        "mathjs": "^15.0.0",
        "monaco-editor": "^0.54.0",
        "patch-package": "^8.0.0",
        "pixi-viewport": "4.24",
        "pixi.js": "6.1.3",
        "process": "^0.11.10",
